{
  "manifest_version": 2,
  "name": "LaterLezer",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "sessions",
    "http://*/*"
  ],
  "description": "LaterLezer extension.",
  "browser_action": {
    "default_icon": "logo192.png",
    "default_title": "LaterLezer",
    "default_popup": "index.html"
  },
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  }
}